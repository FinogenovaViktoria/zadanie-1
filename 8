m = [[1,5,6,7,8],[2,5,6,7,9],[7,5,1,3,6],[8,3,4,9,6],[7,8,7,8,7]]
for i in range (len(m)-1):
    for j in range (len(m[0])):
        m[i][j]- = m [-1][j]     
for i in m :
    print (*i)
