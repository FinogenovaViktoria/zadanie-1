import math
a= float(input("Введите переменную a"));
b= float(input("Введите переменную b"));
if a<b:
  R=(a-b+1)   
  print (R)
if (a>b and a>3.0):
  R=(b**2-1)
  print(R)
else:
  R=(b**2-1)
  print(R)


   
